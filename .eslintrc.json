{
  "extends": ["airbnb-base", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2020 // es11
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars":"warn",
    "no-console":"warn",
    "func-names":"off",
    "no-process-exit":"off",
    "object-shorthand": "off",
    "class-methods-use-this":"off",
    "no-underscore-dangle": "off"
  }
}
